tElementById('result');\n  var totalPrice = 0;\n\n  var countryTable = document.getElementsByClassName('countryTable')[0];\n  var selectedCountry = null;\n\n  var rows = countryTable.getElementsByTagName('tr');\n  for (var i = 0; i < rows.length; i++) {\n    var row = rows[i];\n    if (row.classList.contains('selected')) {\n      selectedCountry = row.getElementsByTagName('td')[0].textContent.trim();\n      break;\n    }\n  }\n\n  if (selectedCountry) {\n    var countryPrice = countryPrices.find(function (price) {\n      return price.country === selectedCountry;\n    });\n\n    if (countryPrice) {\n      var dayButton = document.getElementById('dayButton');\n      var weekButton = document.getElementById('weekButton');\n      var monthButton = document.getElementById('monthButton');\n      var yearlyButton = document.getElementById('YearlyButton');\n\n      if (dayButton.classList.contains('selected')) {\n        totalPrice = countryPrice.dayPrice;\n      } else if (weekButton.classList.contains('selected')) {\n        totalPrice = countryPrice.weekPrice;\n      } else if (monthButton.classList.contains('selected')) {\n        totalPrice = countryPrice.monthPrice;\n      } else if (yearlyButton.classList.contains('selected')) {\n        totalPrice = countryPrice.yearlyPrice;\n      }\n    }\n  }\n\n\n      resultElement.textContent = 'It will cost \u20ac' + totalPrice.toFixed(2);\n    }\n  <\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-inner-column elementor-element elementor-element-4936deb4\" data-id=\"4936deb4\" data-element_type=\"column\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<section class=\"elementor-section elementor-inner-section elementor-element elementor-element-cd568e2 elementor-section-boxed elementor-section-height-default elementor-secti