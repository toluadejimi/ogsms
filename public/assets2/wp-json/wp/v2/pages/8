\r\n  <\/script>\r\n  \r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<section class=\"elementor-section elementor-inner-section elementor-element elementor-element-668fb305 elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"668fb305\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-inner-column elementor-element elementor-element-8380d06\" data-id=\"8380d06\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<div class=\"elementor-element elementor-element-53394b9a elementor-widget__width-auto elementor-widget elementor-widget-html\" data-id=\"53394b9a\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t  <input id=\"ah_number\"  value=\"1\" disabled>\r\n<style>\r\n    #ah_number{\r\n        text-align:center;\r\n        background: transparent;\r\n    border: none;\r\n    font-weight: 600;\r\n    font-size: 21px;\r\n    width: 50px;\r\n    display: inline;\r\n    }\r\n<\/style>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<div class=\"elementor-element elementor-element-708d1b69 elementor-widget elementor-widget-html\" data-id=\"708d1b69\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<script>\nfunction calculateTotalPrice() {\n        \n\/\/ Update the prices here\nconst countryPrices = [\n  {\n    country: 'United Kingdom',\n    dayPrice: 11.99,\n    weekPrice: 18.99,\n    monthPrice: 31.99,\n    yearlyPrice: 345.99\n  },\n  {\n    country: 'Netherlands',\n    dayPrice: 9.99,\n    weekPrice: 14.99,\n    monthPrice: 29.99,\n    yearlyPrice: